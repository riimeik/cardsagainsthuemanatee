<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.layout.StackPane?>
<?import javafx.scene.shape.Circle?>
<?import javafx.scene.text.Font?>
<?import javafx.scene.text.Text?>
<StackPane xmlns:fx="http://javafx.com/fxml/1" maxHeight="-Infinity" maxWidth="-Infinity"
           minHeight="-Infinity"
           minWidth="-Infinity" prefHeight="400.0" prefWidth="600.0"
           xmlns="http://javafx.com/javafx/8">
    <children>
        <Circle fx:id="bottomCircle" fill="WHITE" radius="25.0" stroke="#ffffff00"
                strokeType="INSIDE"/>
        <Text fx:id="letterText" boundsType="VISUAL" fill="#282828" strokeType="OUTSIDE"
              strokeWidth="0.0" text="A" textAlignment="CENTER" textOrigin="CENTER">
            <font>
                <Font name="Segoe UI" size="30.0"/>
            </font>
            <StackPane.margin>
                <Insets right="1.0" top="2.0"/>
            </StackPane.margin>
        </Text>
    </children>
</StackPane>
