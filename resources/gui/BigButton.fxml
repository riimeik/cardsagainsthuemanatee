<?xml version="1.0" encoding="UTF-8"?>


<?import javafx.scene.layout.Pane?>
<?import javafx.scene.layout.StackPane?>
<?import javafx.scene.shape.Rectangle?>
<?import javafx.scene.text.Font?>
<?import javafx.scene.text.Text?>
<Pane xmlns:fx="http://javafx.com/fxml/1" maxHeight="-Infinity" maxWidth="-Infinity"
      minHeight="-Infinity"
      minWidth="-Infinity" prefHeight="45.0" prefWidth="190.0"
      xmlns="http://javafx.com/javafx/8">
    <children>
        <Rectangle fx:id="bottomRectangle" arcHeight="20.0" arcWidth="20.0" fill="WHITE"
                   height="40.0" opacity="0.7" stroke="#7f7f7f" strokeType="INSIDE" width="190.0"
                   y="5.0"/>
        <StackPane fx:id="topLayer" prefHeight="40.0" prefWidth="190.0">
            <children>
                <Rectangle fx:id="topRectangle" arcHeight="20.0" arcWidth="20.0" fill="WHITE"
                           height="40.0" stroke="#6f6f6f" strokeLineCap="ROUND"
                           strokeLineJoin="ROUND" width="190.0"/>
                <Text fx:id="buttonText" strokeType="OUTSIDE" strokeWidth="0.0"
                      textAlignment="CENTER">
                    <font>
                        <Font name="Segoe UI Light" size="16.0"/>
                    </font>
                </Text>
            </children>
        </StackPane>
    </children>
</Pane>
